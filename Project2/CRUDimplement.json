db.restaurants.find({ "isActive": true }, { "restaurantName": 1, "location": 1, "_id": 0 });

--or and gt
db.orders.find({
  $or: [
    { "totalAmount": { $gt: 20 } },
    { "orderStatus": "Preparing" }
  ]
});

--find array queries
db.restaurants.find({ "menu.itemName": "Cheese Fries" });

--updateOne()
